<h1> index.html.erb </h1>
<div ng-controller="ImageController">
<form ng-submit="sendFileRequest">
	<input type="file" name="file" onchange="$scope.sendFileRequest(this.file)"/>
	<input type="submit" id="azure" value="Send">

	<input type="text" ng-model="newEntry.name">
	<input type="submit" value="Send">
</form>
<ul>
	<li ng-repeat="response in responses">
		{{ response.tags }}
	</li>
</ul>
</div>
